<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="507.0" prefWidth="692.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Wallet.TableController">
   <children>
      <TableView id="tableView" fx:id="tableView" prefHeight="443.0" prefWidth="692.0" styleClass="TableStyle" stylesheets="@TableStyle.css">
